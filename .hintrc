{
  "extends": [
    "development"
  ],
  "hints": {
    "disown-opener": "off",
    "meta-viewport": "off",
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off",
        "image-alt": "off"
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id-aria": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "scroll-behavior",
          "aspect-ratio"
        ]
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ]
  }
}